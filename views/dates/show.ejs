<div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 col-md-offset-2">

<% myDate.forEach(function(date){ %>

<div class="row showDate">
  <div class="col-xs-7 col-sm-7 col-md-7 col-lg-7 col-md-offset-2">
  <h1><%= date.title %></h1>
    <div class="well">
      <% date.venues.forEach(function(venue){ %>
        <h3><%= venue.name %></h3>
        <h5><%= venue.address %></h5>
        <h5><%= venue.city %>, <%= venue.state %></h5>
        <h5><%- link_to ('more info',venue.url )  %></h5>
        <br>
        <br>
      <% }); %>
      <a href="/dates" id="add-to-date" type="button" class='btn btn-default'>back</a>
      <a href="<%=date.id%>/search" target="" id="add-to-date" type="button" class='btn btn-default'>add more</a>
    </div>
    <% }); %>
  </div>
</div>
</div>